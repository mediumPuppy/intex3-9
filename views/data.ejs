    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/ag-grid-community/dist/ag-grid-community.min.js"></script>

    <script>
      // Specify the columns for the questions table
      var questionsColumnDefs = [
        { headerName: "Question",
          field: "question"
        },
        { headerName: "Description", 
          field: "description"
        }
      ];
    
      // Data for the questions table
      var questionsRowData = [
        { question: 'Q9', description: 'How often do you use social media without purpose?' },
        { question: 'Q10', description: 'How often are you distracted by social media while busy?' },
        { question: 'Q11', description: 'Do you feel restless when you donâ€™t use social media?' },
        { question: 'Q12', description: 'Are you easily distracted?' },
        { question: 'Q13', description: 'Are you bothered by your worries?' },
        { question: 'Q14', description: 'Is it difficult for you to concentrate?' },
        { question: 'Q15', description: 'How often do you compare yourself to successful people on social media?' },
        { question: 'Q16', description: 'How do you feel about the comparisons from Q15?' },
        { question: 'Q17', description: 'How often do you seek validation from social media?' },
        { question: 'Q18', description: 'How often do you feel depressed or down?' },
        { question: 'Q19', description: 'How frequently does your interest in daily activities fluctuate?' },
        { question: 'Q20', description: 'How often do you have issues regarding sleep?' }
      ];
  
    
      // Initialize AG-Grid for the questions table
      // Create the grid
      var grid = agGrid.createGrid(document.getElementById('questionsGrid'), {
          columnDefs: questionsColumnDefs,
          rowData: questionsRowData,
          onGridReady: function(params) {
              params.api.sizeColumnsToFit(); // size columns to fit the grid width
              var allColumnIds = [];
              params.columnApi.getAllColumns().forEach(function(column) {
                  allColumnIds.push(column.colId);
              });
              params.columnApi.autoSizeColumns(allColumnIds); // auto-size columns based on content
          }
      });

    
      // Specify the columns for the user data table
      var userDataColumnDefs = [
          { headerName: "User_ID", 
            field: "user_id",
            filter: true 
          },
          { headerName: "Timestamp", 
            field: "timestamp",
            filter: true 
          },
          { headerName: "Age", 
            field: "age",
            filter: true 
          },
          { headerName: "Gender", 
            field: "gender",
            filter: true 
          },
          { headerName: "Relationship Status", 
